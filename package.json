{
  "name": "boomboom",
  "version": "1.0.8",
  "description": "",
  "main": "./src/background.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "electron .",
    "server": "webpack-dev-server",
    "dist": "electron-builder --dir --win --x64",
    "package": "electron-packager ./ BoomBoom --out ./OutApp --version 1.0.5 --overwrite --icon=src/assets/red.ico"
  },
 
  "build": {
    "appId": "com.genshin.jumpydumpty",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "src/assets/red.ico",
      "requestedExecutionLevel": "highestAvailable",
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "asar": false,
  "iohook": {
    "targets": [
      "node-72",
      "electron-76"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "keywords": [],
  "author": "ChanIok",
  "license": "ISC",
  "devDependencies": {
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "electron": "^8.5.0",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.5",
    "file-loader": "^6.2.0",
    "less": "^4.1.1",
    "less-loader": "^6.2.0",
    "node-loader": "^1.0.2",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.6",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.19.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.3",
    "axios": "^0.21.1",
    "crypto": "^1.0.1",
    "iohook": "^0.6.6",
    "vkeys": "^1.2.0",
    "vue": "^2.6.12"
  }
}
